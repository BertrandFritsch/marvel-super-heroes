{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": false,
    "interface-name": false,
    "max-line-length": [
      false
    ],
    "member-access": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "one-line": [
      true,
      "check-open-brace"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ]
  },
  "rulesDirectory": []
}