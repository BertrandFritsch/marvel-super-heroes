{
  "name": "marvel-super-heroes",
  "version": "^0.0.1",
  "description": "Marvel Super Heroes",
  "main": "src/index.js",
  "repository": "https://github.com/BertrandFritsch/marvel-super-heroes",
  "author": "bertrand.fritsch.in@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env NODE_ENV=production --env PUBLIC_PATH=/marvel-super-heroes -p",
    "build:watch": "webpack serve --env NODE_ENV=development --progress",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test:unit:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest --coverage",
    "tsc:check": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/webpack": "^4.41.22",
    "@types/webpack-env": "^1.15.3",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.3",
    "node-sass": "^4.14.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^16.14.0",
    "sass-loader": "^10.0.3",
    "source-map-loader": "^1.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.1",
    "ts-lint": "^4.5.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-notifier": "^1.8.0"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "react-stonecutter": "^0.3.10",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|pdf|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "mapCoverage": true
  }
}
