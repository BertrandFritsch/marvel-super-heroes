{
  "env": {
    "test": {
      "sourceMap": "inline",
      "presets": [
        "react",
        "es2015",
        "stage-0"
      ]
    },

    "development": {
      "presets": [
        "react",
        ["es2015", {"modules": false}],
        "stage-0"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ]
    },

    "production": {
      "presets": [
        "react",
        ["es2015", {"modules": false}],
        "stage-0"
      ]
    }
  }
}
